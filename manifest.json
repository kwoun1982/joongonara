{
  "manifest_version": 2,
  "name": "중고나라 검색기",
  "description": "중고나라 검색기",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_idle",
      "js": [
        "vendor/jquery.min.js",
        "vendor/bootstrap.min.js",
        "vendor/jquery.min.js",
        "vendor/underscore.js",
        "view.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "http://*/",
    "activeTab",
    "<all_urls>",
    "notifications",
    "alarms",
    "tabs",
    "\u003Call_urls>",
    "contextMenus",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "storage",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "icon.png",
    "viewer.html"
  ]
}